<p id="notice"><%= notice %></p>

<p>  <strong>Email:</strong>
  <%= @participant.email %></p>
<p>  <strong>Event:</strong>
  <%= @event.id %></p>
<p>  <strong>Locations not visited:</strong>
  <ul>
  <% @event.locations.each do |location| %>
    <% if ! @participant.locations.include? location %>
      <li><%= location.name %></li>
    <% end %>
  <% end %></ul></p>


<%= link_to 'Edit', edit_participant_path(@participant) %> |
<%= link_to 'Back', participants_path %>
